<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.dgit.persistence.BoardDao">
		<insert id="boardInsert">
			INSERT INTO board(board_no,member_no,board_title)VALUES
			(#{boardNo},#{memberNo},#{boardTitle})	
		</insert>
		
		<select id="boardSelect" resultType="Board">
			select board_no,member_no,board_title from board
		</select>
</mapper>